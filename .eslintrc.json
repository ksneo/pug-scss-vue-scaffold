{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "quotes": ["error", "single"],
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": "off",
        "import/prefer-default-export": "off"
    }
}
