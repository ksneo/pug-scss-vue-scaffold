{
  "name": "pizza-django",
  "version": "1.0.0",
  "description": "Pizza-roma site",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:client": "gulp",
    "start:server": "python manage.py runserver 127.0.0.1:8001 --settings pizzajazz.settings.dev",
    "start:docker:win": "docker-service.bat up",
    "build:client": "gulp build-prod",
    "make-block": "babel-node build_scripts/make-block",
    "save:site": "python manage.py dumpdata --natural-foreign --indent 2 -e auth.permission -e auth.group -e auth.user -e admin.logentry -e contenttypes -e wagtailcore.GroupCollectionPermission -e wagtailcore.pagerevision -e wagtailimages.rendition -e sessions -e pjdel -e background_task > ./pjsite/fixtures/pjsite.json",
    "backup:db": "docker exec db_pizzajazz /usr/bin/mysqldump -u pizzajazz pizzajazz > ./database_backup/db_backup.sql",
    "convert:scripts": "dos2unix ./scripts/*.sh",
    "docker:bash": "docker exec -it pizza_django bash"
  },
  "repository": {
    "type": "git",
    "url": "git+https://vkashapov@bitbucket.org/aleoreh/pizza-django.git"
  },
